{
    "name": "stfalcon-studio/swagger-bundle",
    "description": "Generate Swagger UI documentation from local files",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "4.2.*",
        "symfony/twig-bundle": "^4.0",
        "symfony/yaml": "4.2.*",
        "symfony/asset": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/polyfill-ctype": "^1.11",
        "symfony/templating": "4.2.*"
    },
    "autoload": {
        "psr-4": {
            "StfalconStudio\\SwaggerBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StfalconStudio\\SwaggerBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
